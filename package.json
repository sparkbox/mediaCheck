{
  "name": "mediacheck",
  "version": "1.0.0",
  "homepage": "http://github.com/sparkbox/mediaCheck",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "posttest": "snyk test",
    "test:watch": "npm test -- -w",
    "start": "run-p build:watch test:watch",
    "build": "webpack",
    "build:watch": "webpack -w",
    "postversion": "npm run dist",
    "deprecated-dist": "npm run build && npm run uglify",
    "deprecated-uglify": "uglifyjs ./js/mediaCheck.js > ./js/mediaCheck-min.js"
  },
  "main": "./lib/mediaCheck.js",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "snyk": "^1.19.1",
    "uglify-js": "^2.7.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  }
}
