{
  "name": "mediacheck",
  "version": "1.0.0",
  "homepage": "http://github.com/sparkbox/mediaCheck",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "posttest": "snyk test",
    "test:watch": "npm test -- -w",
    "start": "npm run test:watch",
    "build": "rollup && uglifyjs -o lib/mediaCheck-bundle.min.js lib/mediaCheck-bundle.js",
    "postversion": "npm run build"
  },
  "main": "./lib/mediaCheck-bundle.min.js",
  "module": "./src/mediaCheck.js",
  "jsnext:main": "./src/mediaCheck.js",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "snyk": "^1.19.1",
    "uglify-js": "^2.7.3"
  },
  "dependencies": {
    "rollup": "^0.66.5"
  }
}
